          <Formula type="Products.Archetypes.Field.TextField"><![CDATA[#NB il documento viene renderizzato usando la form specificata in settings/labelcolumn
db=context.getParentDatabase()
if not context.getParentDatabase().resources.customscripts.getPagoPaEnabled(context):
    return

renderForm=db.getForm('base_sub_pagopa')
content=asUnicode(renderForm.displayDocument(context))

#aggiungo i doc di pagamenti inviati 
pagamenti=''
idx = db.getIndex()
brains = idx.dbsearch(dict(numero_pratica=context.getItem('numero_pratica'),Form='frm_invio_pagamento'),sortindex="data_pratica")
for brain in brains:
    doc=brain.getObject()
    pagamenti = pagamenti + '<p><h5><a target="_blank" href="%s"> %s </a></h5></p>'%(doc.absolute_url(),doc.getId())
if pagamenti:
    pagamenti='<fieldset class="scheduler-border"><legend class="scheduler-border"> Pagamenti inviati </legend>%s</fieldset>'%(pagamenti)

html=''
if content:
    html='<fieldset class="scheduler-border"><legend class="scheduler-border"><a href="%s/EditDocument?openwithform=base_sub_pagopa"> Importi e pagamenti <i class="icon-edit"></i></a></legend> %s %s </fieldset>'%(context.absolute_url(),content,pagamenti)


       
return html]]></Formula>
