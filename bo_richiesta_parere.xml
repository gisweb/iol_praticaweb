        <element id="parere_risposte_elenco" title="Elenco pareri acquisiti" type="PlominoField">
          <id type="Products.Archetypes.Field.StringField">parere_risposte_elenco</id>
          <FieldType type="Products.Archetypes.Field.StringField">TEXT</FieldType>
          <FieldMode type="Products.Archetypes.Field.StringField">DISPLAY</FieldMode>
          <Formula type="Products.Archetypes.Field.TextField"><![CDATA[from iol.document.plomino_utils import getSelectionValue

db=context.getParentDatabase()
idx = db.getIndex()
if not 'parent_document' in idx.indexes():
    return ''

if context.isEditMode():
    return ''


brains = idx.dbsearch(dict(parent_document=context.getId(),Form=['bo_risposta_parere']),sortindex='id')  
html=''

for brain in brains:
    doc=brain.getObject()
    editForm = 'bo_risposta_parere'
    ente = getSelectionValue(doc,'parere_risposta_opt',editForm)
    renderForm=db.getForm(editForm)
    content=asUnicode(renderForm.displayDocument(doc))
    if content:
        html=html+'<fieldset class="scheduler-border"><legend class="scheduler-border"><a href="%s/EditDocument?openwithform=%s"> Parere acquisito da %s <i class="icon-edit"></i></a></legend>%s</fieldset>'%(doc.absolute_url(),editForm,ente,content)

    
return html]]></Formula>
